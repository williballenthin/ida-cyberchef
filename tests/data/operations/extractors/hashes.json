{
  "operation": "Extract hashes",
  "category": "extractors",
  "description": "Tests for extracting hash values (MD5, SHA1, SHA256) from text based on character length",
  "tests": [
    {
      "name": "extract_md5_hash",
      "comment": "Extract MD5 hash (32 characters)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "4d44352068617368203564343134303261626334623261373662393731396439313130313763353932"
      },
      "operations": [
        {
          "op": "Extract hashes",
          "args": {
            "Hash character length": 32
          }
        }
      ],
      "expected": {
        "type": "string",
        "value": "5d41402abc4b2a76b9719d911017c592"
      },
      "tags": [
        "extractor",
        "hash",
        "md5"
      ]
    },
