{
  "operation": "Parse UDP",
  "category": "networking",
  "description": "Parse UDP header tests for various UDP configurations",
  "tests": [
    {
      "name": "parse_udp_dns",
      "comment": "Parse UDP header for DNS traffic (port 53)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "0035C35000080000"
      },
      "operations": [
        "Parse UDP"
      ],
      "expected": {
        "type": "string",
        "value": "53"
      },
      "tags": [
        "udp",
        "dns"
      ]
    },
    {
      "name": "parse_udp_dhcp_client",
      "comment": "Parse UDP header for DHCP client traffic (port 68)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "0044004300080000"
      },
      "operations": [
        "Parse UDP"
      ],
      "expected": {
        "type": "string",
        "value": "68"
      },
      "tags": [
        "udp",
        "dhcp"
      ]
    },
    {
      "name": "parse_udp_ntp",
