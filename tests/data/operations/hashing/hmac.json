{
  "operation": "HMAC",
  "category": "hashing",
  "description": "HMAC (Keyed-Hash Message Authentication Code) tests with various hash functions. Note: Key parameter expects hex-encoded bytes.",
  "tests": [
    {
      "name": "hmac_md5_secret_hello",
      "comment": "HMAC-MD5 with key 'secret' and message 'hello' - verified against Python hmac module",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f"
      },
      "operations": [
        {
          "op": "HMAC",
          "args": {
            "Key": "736563726574",
            "Hashing function": "MD5"
          }
        }
      ],
      "expected": {
        "type": "string",
        "value": "bade63863c61ed0b3165806ecd6acefc"
      },
      "tags": [
        "hmac-md5"
      ]
    },
