{
  "operation": "MD2",
  "category": "hashing",
  "description": "MD2 hash algorithm tests with RFC 1319 standard test vectors",
  "tests": [
    {
      "name": "empty",
      "comment": "MD2 of empty string - RFC 1319 test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": [
        "MD2"
      ],
      "expected": {
        "type": "string",
        "value": "8350e5a3e24c153df2275c9f80692773"
      },
      "tags": [
        "rfc-1319",
        "standard-vector",
        "empty"
      ]
    },
    {
      "name": "single_a",
      "comment": "MD2 of single character 'a' - RFC 1319 test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "61"
      },
      "operations": [
        "MD2"
      ],
      "expected": {
        "type": "string",
        "value": "32ec01ec4a6dac72c0ab96fb34c0b5d1"
      },
      "tags": [
        "rfc-1319",
        "standard-vector"
      ]
    },
    {
      "name": "abc",
      "comment": "MD2 of 'abc' - RFC 1319 test vector",
