{
  "operation": "MD5",
  "category": "hashing",
  "description": "MD5 hash algorithm tests with standard test vectors",
  "tests": [
    {
      "name": "empty",
      "comment": "MD5 of empty string - standard test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": [
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "d41d8cd98f00b204e9800998ecf8427e"
      },
      "tags": [
        "standard-vector",
        "empty"
      ]
    },
    {
      "name": "single_a",
      "comment": "MD5 of single character 'a' - standard test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "61"
      },
      "operations": [
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "0cc175b9c0f1b6a831c399e269772661"
      },
      "tags": [
        "standard-vector"
      ]
    },
    {
      "name": "abc",
      "comment": "MD5 of 'abc' - standard test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "616263"
      },
      "operations": [
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "900150983cd24fb0d6963f7d28e17f72"
      },
      "tags": [
        "standard-vector"
      ]
    },
    {
      "name": "message_digest",
      "comment": "MD5 of 'message digest' - standard test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "6d65737361676520646967657374"
      },
      "operations": [
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "f96b697d7cb7938d525a2f31aaf161d0"
      },
      "tags": [
        "standard-vector"
      ]
    },
    {
      "name": "hello",
      "comment": "MD5 of 'hello' - standard test vector",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f"
      },
      "operations": [
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "5d41402abc4b2a76b9719d911017c592"
      },
      "tags": [
        "standard-vector"
      ]
    },
    {
      "name": "all_bytes",
      "comment": "MD5 of all 256 byte values (0x00-0xFF)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": [
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "e2c865db4162bed963bfaa9ef6ac18f0"
      },
      "tags": [
        "all-bytes"
      ]
    },
    {
      "name": "double_md5",
      "comment": "Double MD5 hashing - MD5(MD5('hello'))",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f"
      },
      "operations": [
        "MD5",
        "MD5"
      ],
      "expected": {
        "type": "string",
        "value": "69a329523ce1ec88bf63061863d9cb14"
      },
      "tags": [
        "chaining"
      ]
    }
  ]
}