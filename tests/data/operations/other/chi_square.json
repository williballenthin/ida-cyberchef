{
  "operation": "Chi Square",
  "category": "other",
  "description": "Chi Square distribution tests for randomness detection",
  "tests": [
    {
      "name": "chi_square_uniform_bytes",
      "comment": "Chi Square of perfectly uniform bytes (each value 0-255 once)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Chi Square"],
      "expected": {
        "type": "string",
        "value": "0"
      },
      "tags": ["chi-square", "uniform"]
    },
    {
      "name": "chi_square_all_zeros",
      "comment": "Chi Square of all zero bytes (maximum non-randomness)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "00000000000000000000000000000000"
      },
      "operations": ["Chi Square"],
      "expected": {
        "type": "string",
        "value": "3840"
      },
      "tags": ["chi-square", "non-random"]
    },
    {
      "name": "chi_square_all_same",
      "comment": "Chi Square of repeated same byte",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      "operations": ["Chi Square"],
      "expected": {
        "type": "string",
        "value": "3840"
      },
      "tags": ["chi-square", "repeated"]
    },
    {
      "name": "chi_square_single_byte",
      "comment": "Chi Square with minimal input (single byte)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "ff"
      },
      "operations": ["Chi Square"],
      "expected": {
        "type": "string",
        "value": "255"
      },
      "tags": ["chi-square", "edge-case"]
    }
  ]
}
