{
  "operation": "AES Key Wrap/Unwrap",
  "category": "encryption",
  "description": "Tests for AES Key Wrap and Key Unwrap using RFC 3394 test vectors",
  "tests": [
    {
      "name": "aes_128_key_wrap",
      "comment": "AES-128 Key Wrap using RFC 3394 test vector with 128-bit KEK",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "00112233445566778899aabbccddeeff"
      },
      "operations": [
        {
          "op": "AES Key Wrap",
          "args": {
            "Key (KEK)": {
              "option": "Hex",
              "string": "000102030405060708090a0b0c0d0e0f"
            },
            "IV": {
              "option": "Hex",
              "string": "a6a6a6a6a6a6a6a6"
            },
            "Input": "Hex",
            "Output": "Hex"
          }
        }
      ],
      "expected": {
        "type": "string",
        "value": "1fa68b0a8112b447aef34bd8fb5a7b829d3e862371d2cfe5"
      },
      "tags": ["aes-128", "key-wrap", "rfc3394"]
    },
    {
      "name": "aes_128_key_unwrap",
      "comment": "AES-128 Key Unwrap using RFC 3394 test vector with 128-bit KEK",
