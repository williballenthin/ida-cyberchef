{
  "operation": "Cetacean Cipher",
  "category": "encryption",
  "description": "Tests for Cetacean Cipher Encode and Decode operations. Cetacean cipher encodes text using 'E' and 'e' characters to represent binary data.",
  "tests": [
    {
      "name": "encode_basic_lowercase",
      "comment": "Encode lowercase 'hi' to Cetacean cipher (example from CyberChef)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "6869"
      },
      "operations": ["Cetacean Cipher Encode"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "4545454545454545456565456545454545454545454545454565654565454565"
      },
      "tags": ["encode", "basic", "lowercase"]
    },
    {
      "name": "decode_basic_lowercase",
      "comment": "Decode Cetacean cipher back to 'hi'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "4545454545454545456565456545454545454545454545454565654565454565"
      },
      "operations": ["Cetacean Cipher Decode"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "6869"
      },
      "tags": ["decode", "basic"]
    },
    {
      "name": "encode_uppercase",
      "comment": "Encode uppercase 'HI' to Cetacean cipher",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "4849"
      },
      "operations": ["Cetacean Cipher Encode"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "4545454545454545654565454545454545454545654565456565456545454565"
      },
      "tags": ["encode", "uppercase"],
      "skip": true,
      "skipReason": "Expected value calculated - needs verification against actual CyberChef output"
    },
    {
      "name": "encode_single_char",
      "comment": "Encode single character 'A' to Cetacean cipher",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "41"
      },
      "operations": ["Cetacean Cipher Encode"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "45454545454545456545454565454565"
      },
      "tags": ["encode", "single-char"],
      "skip": true,
      "skipReason": "Expected value calculated - needs verification against actual CyberChef output"
    },
    {
      "name": "roundtrip_test",
      "comment": "Encode 'test' and verify roundtrip by decoding",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "74657374"
      },
      "operations": ["Cetacean Cipher Encode", "Cetacean Cipher Decode"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "74657374"
      },
      "tags": ["roundtrip", "encode", "decode"]
    },
    {
      "name": "encode_empty",
      "comment": "Encode empty input should produce empty output",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Cetacean Cipher Encode"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["edge-case", "empty-input"]
    }
  ]
}
