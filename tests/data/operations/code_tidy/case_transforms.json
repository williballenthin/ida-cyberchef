{
  "operation": "Case transformations",
  "category": "code_tidy",
  "description": "Tests for camel case, kebab case, and snake case transformations",
  "tests": [
    {
      "name": "to_camel_case_basic",
      "comment": "Convert space-separated words to camelCase",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f20776f726c64"
      },
      "operations": [
        "To Camel case"
      ],
      "expected": {
        "type": "string",
        "value": "helloWorld"
      },
      "tags": [
        "camel",
        "basic"
      ]
    },
    {
      "name": "to_camel_case_from_title",
      "comment": "Convert title case to camelCase",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f20576f726c64"
      },
