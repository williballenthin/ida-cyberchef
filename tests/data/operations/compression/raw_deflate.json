{
  "operation": "Raw Deflate",
  "category": "compression",
  "description": "Tests for Raw Deflate and Raw Inflate operations (deflate without zlib wrapper)",
  "tests": [
    {
      "name": "raw_inflate_hello_world",
      "comment": "Decompress raw deflate data (zlib output without header/checksum) to 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "f348cdc9c9d75108cf2fca49510400"
      },
      "operations": ["Raw Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["decompress", "python-interop"]
    },
    {
      "name": "raw_deflate_roundtrip_hello_world",
      "comment": "Test Raw Deflate/Inflate roundtrip with 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": ["Raw Deflate", "Raw Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip"]
    },
    {
      "name": "raw_deflate_roundtrip_all_bytes",
      "comment": "Test Raw Deflate roundtrip with all 256 possible byte values",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Raw Deflate", "Raw Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "binary"]
    },
    {
      "name": "raw_deflate_roundtrip_utf8_simple",
      "comment": "Test Raw Deflate roundtrip with UTF-8 text",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "operations": ["Raw Deflate", "Raw Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "tags": ["compress", "roundtrip", "utf8"]
    },
    {
      "name": "raw_deflate_roundtrip_empty",
      "comment": "Test Raw Deflate roundtrip with empty input",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Raw Deflate", "Raw Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "empty", "edge-case"]
    },
    {
      "name": "raw_deflate_compression_type_dynamic",
      "comment": "Test Raw Deflate with dynamic Huffman coding and verify with Inflate",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Raw Deflate",
          "args": {
            "Compression type": "Dynamic Huffman Coding"
          }
        },
        "Raw Inflate"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "raw_deflate_compression_type_fixed",
      "comment": "Test Raw Deflate with fixed Huffman coding and verify with Inflate",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Raw Deflate",
          "args": {
            "Compression type": "Fixed Huffman Coding"
          }
        },
        "Raw Inflate"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "raw_deflate_compression_type_none",
      "comment": "Test Raw Deflate with no compression (store only) and verify with Inflate",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Raw Deflate",
          "args": {
            "Compression type": "None (Store)"
          }
        },
        "Raw Inflate"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    }
  ]
}
