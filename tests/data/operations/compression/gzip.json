{
  "operation": "Gzip/Gunzip",
  "category": "compression",
  "description": "Tests for Gzip compression and Gunzip decompression operations",
  "tests": [
    {
      "name": "gunzip_hello_world",
      "comment": "Decompress Python gzip.compress() output to 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "1f8b080004ce566902fff348cdc9c9d75108cf2fca49510400d0c34aec0d000000"
      },
      "operations": ["Gunzip"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["decompress", "python-interop"]
    },
    {
      "name": "gunzip_empty",
      "comment": "Decompress gzip-compressed empty data",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "1f8b080004ce566902ff03000000000000000000"
      },
      "operations": ["Gunzip"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["decompress", "empty", "edge-case"]
    },
    {
      "name": "gzip_compression_type_dynamic",
      "comment": "Test Gzip with dynamic Huffman coding and verify with Gunzip",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Gzip",
          "args": {
            "Compression type": "Dynamic Huffman Coding"
          }
        },
        "Gunzip"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "gzip_compression_type_fixed",
      "comment": "Test Gzip with fixed Huffman coding and verify with Gunzip",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Gzip",
          "args": {
            "Compression type": "Fixed Huffman Coding"
          }
        },
        "Gunzip"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "gzip_compression_type_none",
      "comment": "Test Gzip with no compression (store only) and verify with Gunzip",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Gzip",
          "args": {
            "Compression type": "None (Store)"
          }
        },
        "Gunzip"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "gzip_gunzip_all_bytes",
      "comment": "Test Gzip/Gunzip roundtrip with all 256 possible byte values",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Gzip", "Gunzip"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "binary"]
    },
    {
      "name": "gzip_gunzip_utf8_simple",
      "comment": "Test Gzip/Gunzip roundtrip with UTF-8 text including Chinese and emoji",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "operations": ["Gzip", "Gunzip"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "tags": ["compress", "roundtrip", "utf8"]
    },
    {
      "name": "gzip_gunzip_empty",
      "comment": "Test Gzip/Gunzip roundtrip with empty input",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Gzip", "Gunzip"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "empty", "edge-case"]
    },
    {
      "name": "gzip_gunzip_null_bytes",
      "comment": "Test Gzip/Gunzip roundtrip with data containing null bytes",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f000000776f726c640000007465737400"
      },
      "operations": ["Gzip", "Gunzip"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f000000776f726c640000007465737400"
      },
      "tags": ["compress", "roundtrip", "null-bytes", "edge-case"]
    }
  ]
}
