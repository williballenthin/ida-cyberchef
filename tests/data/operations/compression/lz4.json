{
  "operation": "LZ4",
  "category": "compression",
  "description": "Tests for LZ4 compression and decompression operations",
  "tests": [
    {
      "name": "lz4_roundtrip_hello_world",
      "comment": "Test LZ4 Compress/Decompress roundtrip with 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": ["LZ4 Compress", "LZ4 Decompress"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip"]
    },
    {
      "name": "lz4_roundtrip_empty",
      "comment": "Test LZ4 roundtrip with empty input",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["LZ4 Compress", "LZ4 Decompress"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "empty", "edge-case"]
    },
    {
      "name": "lz4_roundtrip_all_bytes",
      "comment": "Test LZ4 roundtrip with all 256 possible byte values",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["LZ4 Compress", "LZ4 Decompress"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "binary"]
    },
    {
      "name": "lz4_roundtrip_utf8_simple",
      "comment": "Test LZ4 roundtrip with UTF-8 text",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "operations": ["LZ4 Compress", "LZ4 Decompress"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "tags": ["compress", "roundtrip", "utf8"]
    },
    {
      "name": "lz4_roundtrip_utf8_emoji",
      "comment": "Test LZ4 roundtrip with UTF-8 emoji",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "f09f8e89f09f9a80f09f92bbf09f94a5e2ad90"
      },
      "operations": ["LZ4 Compress", "LZ4 Decompress"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "f09f8e89f09f9a80f09f92bbf09f94a5e2ad90"
      },
      "tags": ["compress", "roundtrip", "utf8"]
    },
    {
      "name": "lz4_roundtrip_utf8_multilang",
      "comment": "Test LZ4 roundtrip with UTF-8 multilingual text",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f20d985d8b1d8add8a8d8a720e38193e38293e381abe381a1e381af20ec9588eb8595ed9598ec84b8ec9a9420d09fd180d0b8d0b2d0b5d182"
      },
      "operations": ["LZ4 Compress", "LZ4 Decompress"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f20d985d8b1d8add8a8d8a720e38193e38293e381abe381a1e381af20ec9588eb8595ed9598ec84b8ec9a9420d09fd180d0b8d0b2d0b5d182"
      },
      "tags": ["compress", "roundtrip", "utf8"]
    }
  ]
}
