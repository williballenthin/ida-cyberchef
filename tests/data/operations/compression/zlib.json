{
  "operation": "Zlib",
  "category": "compression",
  "description": "Tests for Zlib Deflate and Inflate operations",
  "tests": [
    {
      "name": "zlib_inflate_hello_world",
      "comment": "Decompress Python zlib.compress() output to 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "789cf348cdc9c9d75108cf2fca495104001f9e046a"
      },
      "operations": ["Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["decompress", "python-interop"]
    },
    {
      "name": "zlib_inflate_empty",
      "comment": "Decompress zlib-compressed empty data",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "789c030000000001"
      },
      "operations": ["Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["decompress", "empty", "edge-case"]
    },
    {
      "name": "zlib_roundtrip_hello_world",
      "comment": "Test Zlib Deflate/Inflate roundtrip with 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": ["Zlib Deflate", "Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip"]
    },
    {
      "name": "zlib_roundtrip_all_bytes",
      "comment": "Test Zlib roundtrip with all 256 possible byte values",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Zlib Deflate", "Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "binary"]
    },
    {
      "name": "zlib_roundtrip_utf8_simple",
      "comment": "Test Zlib roundtrip with UTF-8 text",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "operations": ["Zlib Deflate", "Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "tags": ["compress", "roundtrip", "utf8"]
    },
    {
      "name": "zlib_roundtrip_empty",
      "comment": "Test Zlib roundtrip with empty input",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": ["Zlib Deflate", "Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "tags": ["compress", "roundtrip", "empty", "edge-case"]
    },
    {
      "name": "zlib_deflate_compression_type_dynamic",
      "comment": "Test Zlib Deflate with dynamic Huffman coding and verify with Inflate",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Zlib Deflate",
          "args": {
            "Compression type": "Dynamic Huffman Coding"
          }
        },
        "Zlib Inflate"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "zlib_deflate_compression_type_fixed",
      "comment": "Test Zlib Deflate with fixed Huffman coding and verify with Inflate",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Zlib Deflate",
          "args": {
            "Compression type": "Fixed Huffman Coding"
          }
        },
        "Zlib Inflate"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "zlib_deflate_compression_type_none",
      "comment": "Test Zlib Deflate with no compression (store only) and verify with Inflate",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "Zlib Deflate",
          "args": {
            "Compression type": "None (Store)"
          }
        },
        "Zlib Inflate"
      ],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "tags": ["compress", "roundtrip", "compression-options"]
    },
    {
      "name": "zlib_roundtrip_null_bytes",
      "comment": "Test Zlib roundtrip with data containing null bytes",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f000000776f726c640000007465737400"
      },
      "operations": ["Zlib Deflate", "Zlib Inflate"],
      "expected": {
        "type": "bytes",
        "encoding": "hex",
        "value": "68656c6c6f000000776f726c640000007465737400"
      },
      "tags": ["compress", "roundtrip", "null-bytes", "edge-case"]
    }
  ]
}
