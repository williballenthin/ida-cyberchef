{
  "operation": "LZString",
  "category": "compression",
  "description": "Tests for LZString compression and decompression operations",
  "tests": [
    {
      "name": "lzstring_roundtrip_hello_world_utf16",
      "comment": "Test LZString Compress/Decompress roundtrip with 'Hello, World!' using UTF16 format (default)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        "LZString Compress",
        "LZString Decompress"
      ],
      "expected": {
        "type": "string",
        "value": "Hello, World!"
      },
      "tags": [
        "compress",
        "roundtrip",
        "string-output"
      ]
    },
    {
      "name": "lzstring_roundtrip_hello_world_base64",
      "comment": "Test LZString with Base64 format for 'Hello, World!'",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c6421"
      },
      "operations": [
        {
          "op": "LZString Compress",
          "args": {
            "Compression Format": "Base64"
          }
        },
        {
          "op": "LZString Decompress",
          "args": {
            "Compression Format": "Base64"
          }
        }
      ],
      "expected": {
        "type": "string",
        "value": "Hello, World!"
      },
      "tags": [
        "compress",
        "roundtrip",
        "string-output",
        "compression-options"
      ]
    },
    {
      "name": "lzstring_roundtrip_empty",
      "comment": "Test LZString roundtrip with empty input",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
      },
      "operations": [
        "LZString Compress",
        "LZString Decompress"
      ],
      "expected": {
        "type": "string",
        "value": ""
      },
      "tags": [
        "compress",
        "roundtrip",
        "empty",
        "edge-case",
        "string-output"
      ]
    },
    {
      "name": "lzstring_roundtrip_all_bytes_base64",
      "comment": "Test LZString roundtrip with all 256 bytes using Base64 format (latin-1 encodable)",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": ""
