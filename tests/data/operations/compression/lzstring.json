      "operations": [
        {
          "op": "LZString Compress",
          "args": {
            "Compression Format": "Base64"
          }
        },
        {
          "op": "LZString Decompress",
          "args": {
            "Compression Format": "Base64"
          }
        }
      ],
      "expected": {
        "type": "string",
        "encoding": "latin-1",
        "value": ""
      },
      "tags": [
        "compress",
        "roundtrip",
        "binary",
        "string-output",
        "compression-options"
      ],
      "skip": true,
      "skipReason": "Test expected value truncated or incorrect"
    },
    {
      "name": "lzstring_roundtrip_utf8_simple",
      "comment": "Test LZString roundtrip with UTF-8 text using UTF16 format",
      "input": {
        "type": "bytes",
        "encoding": "hex",
        "value": "48656c6c6f2c20576f726c642120e4bda0e5a5bde4b896e7958c20f09f8c8d"
      },
      "operations": [
        "LZString Compress",
