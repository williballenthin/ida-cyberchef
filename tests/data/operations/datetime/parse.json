{
  "operation": "Parse DateTime",
  "category": "datetime",
  "description": "Parse DateTime strings and display detailed information",
  "tests": [
    {
      "name": "parse_datetime_epoch",
      "comment": "Parse Unix epoch date",
      "input": {
        "type": "string",
        "value": "01/01/1970 00:00:00"
      },
      "operations": [
        {
          "op": "Parse DateTime",
          "args": [
            "DD/MM/YYYY HH:mm:ss",
            "UTC"
          ]
        }
      ],
      "expected": {
        "type": "string",
        "value": "1970"
      },
      "tags": [
        "datetime",
        "parse",
        "epoch"
      ]
    },
    {
      "name": "parse_datetime_y2k",
      "comment": "Parse Y2K date",
      "input": {
        "type": "string",
        "value": "01/01/2000 00:00:00"
      },
      "operations": [
        {
          "op": "Parse DateTime",
          "args": [
            "DD/MM/YYYY HH:mm:ss",
            "UTC"
          ]
        }
      ],
      "expected": {
        "type": "string",
        "value": "2000"
      },
      "tags": [
        "datetime",
        "parse",
        "y2k"
      ]
    },
    {
      "name": "parse_datetime_iso_format",
      "comment": "Parse ISO 8601 format date",
      "input": {
        "type": "string",
        "value": "2023-06-15 14:30:00"
      },
      "operations": [
        {
          "op": "Parse DateTime",
          "args": [
            "YYYY-MM-DD HH:mm:ss",
            "UTC"
          ]
        }
      ],
      "expected": {
        "type": "string",
        "value": "2023"
      },
      "tags": [
        "datetime",
        "parse",
        "iso8601"
      ]
    },
    {
      "name": "parse_datetime_american",
      "comment": "Parse American-style date format",
      "input": {
        "type": "string",
        "value": "12/31/2022 23:59:59"
      },
      "operations": [
        {
          "op": "Parse DateTime",
          "args": [
            "MM/DD/YYYY HH:mm:ss",
            "UTC"
          ]
        }
      ],
      "expected": {
        "type": "string",
        "value": "2022"
      },
      "tags": [
        "datetime",
        "parse"
      ]
    },
    {
      "name": "parse_datetime_unix_timestamp",
      "comment": "Parse Unix timestamp directly",
      "input": {
        "type": "string",
        "value": "946684800"
      },
      "operations": [
        {
          "op": "Parse DateTime",
          "args": [
            "X",
            "UTC"
          ]
        }
      ],
      "expected": {
        "type": "string",
        "value": "2000"
      },
      "tags": [
        "datetime",
        "parse",
        "unix_timestamp",
        "y2k"
      ]
    }
  ]
}
